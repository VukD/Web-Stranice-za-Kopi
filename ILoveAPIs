<script type="text/javascript">
jQuery(document).ready(function() {
	jQuery('.box').matchHeight();

	function loadTitle() {
	    jQuery('.intro-headline').removeClass('loadTitle');
	}

	setTimeout(loadTitle, 1500);

	jQuery('.pick-day').click(function() {
		var agenda1 = jQuery('#aq-block-1445-12');
		var agenda2 = jQuery('#aq-block-1445-13');
		var agenda3 = jQuery('#aq-block-1445-14');

		if(jQuery(this).attr("id") == 'day1') { agenda2.hide(); agenda3.hide(); agenda1.show();  }
		else if(jQuery(this).attr("id") == 'day2') { agenda2.show(); agenda3.hide(); agenda1.hide();}
		else if(jQuery(this).attr("id") == 'day3') { agenda2.hide(); agenda3.show(); agenda1.hide(); }

		jQuery(this).parent().siblings().each(function() {
			jQuery(this).children('.pick-day').removeClass('is-active');
		});

		jQuery(this).addClass('is-active');
	});

	jQuery('#status').fadeOut(); // will first fade out the loading animation
	jQuery('#preloader').delay(350).fadeOut('slow'); // will fade out the white DIV that covers the website.
	jQuery(window).trigger('resize');

	var video = document.getElementById('full-video');
    video.addEventListener('loadeddata', function() {
	   	jQuery('.orange-overlay').fadeIn();
		jQuery('#full-video').fadeIn();
	}, false);

});
</script>

